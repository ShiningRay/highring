    "swim",
    "gossip",
    "streams"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/upringjs/upring.git"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
